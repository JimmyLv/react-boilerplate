{
  "name": "react-boilerplate",
  "version": "1.0.0",
  "description": "Just an boilerplate by using nobackend tech stack.",
  "main": "./src/index.jsx",
  "scripts": {
    "clean": "rm -rf dist && rm -rf _site",
    "serve": "webpack-dev-server",
    "build:pwa": "cp favicon.ico cache-polyfill.js dist/ && sw-precache --navigate-fallback='index.html' --root='dist/' --static-file-globs='dist/**/!(*map*)'",
    "build:webpack": "cross-env NODE_ENV=production webpack --progress --colors --hide-modules",
    "build:stats": "webpack --profile --json > stats.json",
    "build": "npm run clean && npm run build:webpack",
    "lint": "eslint src/. --ext .jsx --ext .js; exit 0",
    "lint:flow": "flow; test $? -eq 0 -o $? -eq 2",
    "lint:fix": "eslint --fix src/. --ext .jsx --ext .js; exit 0",
    "test": "karma start --single-run=true",
    "push": "cd dist; git init; git add -A; timestamp=$(date \"+%c\") && git commit -m \"Automated deployment at $timestamp\"; git push -f https://github.com/JimmyLv/react-boilerplate.git master:gh-pages",
    "deploy": "npm run build && npm run push",
    "start": "npm run clean && npm run build && jekyll serve"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JimmyLv/react-boilerplate"
  },
  "homepage": "http://haiku.jimmylv.info/",
  "author": "Jimmy Lv",
  "license": "ISC",
  "dependencies": {
    "classnames": "^2.2.5",
    "history": "^3.0.0",
    "lodash": "^4.16.6",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-redux": "^4.4.5",
    "react-redux-loading-bar": "^2.4.0",
    "react-router": "^2.8.1",
    "react-router-redux": "^4.0.6",
    "redux": "^3.6.0",
    "redux-logger": "^2.7.0",
    "redux-saga": "^0.11.1",
    "whatwg-fetch": "^1.0.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.5.1",
    "babel-cli": "^6.16.0",
    "babel-core": "^6.17.0",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-async-to-generator": "^6.16.0",
    "babel-plugin-transform-class-properties": "^6.16.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-es2016": "^6.16.0",
    "babel-preset-es2017": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-runtime": "^6.11.6",
    "chai": "^3.5.0",
    "chai-enzyme": "^0.5.0",
    "copy-webpack-plugin": "^3.0.1",
    "cross-env": "^1.0.8",
    "css-loader": "^0.23.1",
    "enzyme": "^2.4.1",
    "eslint": "^3.9.1",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-import-resolver-webpack": "^0.3.0",
    "eslint-plugin-flowtype": "^2.20.0",
    "eslint-plugin-flowtype-errors": "^1.4.1",
    "eslint-plugin-immutable": "^1.0.0",
    "eslint-plugin-import": "^1.9.2",
    "eslint-plugin-jsx-a11y": "^1.5.3",
    "eslint-plugin-react": "^5.2.2",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "flow-bin": "^0.32.0",
    "happypack": "^2.2.1",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.22.0",
    "istanbul-instrumenter-loader": "^0.2.0",
    "json-loader": "^0.5.4",
    "karma": "^1.3.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^1.0.1",
    "karma-coverage": "^1.1.1",
    "karma-mocha": "^1.2.0",
    "karma-mocha-reporter": "^2.2.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.8.0",
    "less": "^2.6.1",
    "less-loader": "^2.2.2",
    "mocha": "^2.5.3",
    "postcss-loader": "^0.8.2",
    "precss": "^1.4.0",
    "raw-loader": "^0.5.1",
    "react-hot-loader": "^1.3.0",
    "style-loader": "^0.13.0",
    "sw-precache": "^4.1.0",
    "sw-precache-webpack-plugin": "^0.5.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.16.2"
  }
}
